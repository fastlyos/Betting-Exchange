<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Css/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Content/Scripts/JQuery/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/Microsoft/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
        @Html.Partial("_Header")
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        @Html.Partial("_Footer")
        </footer>
    </div>
</body>
</html>
